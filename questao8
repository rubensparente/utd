<?php

	$idade = array("12", "19", "20", "8", "30");
	$menor = 0;
	$maior = 0;

	echo "Idades<br>";
	for($i=0; $i<count($idade);$i++){
		echo $idade[$i]."<br>";
		if($idade[$i] < 18){
			$menor++;
		}
		else{
			$maior++;
		}
	}
	echo "Resultado<br>";
	echo $menor. " pessoas são menores de 18 anos.<br>";
	echo $maior. " pessoas são maiores de 18 anos.";
?>
